<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:backgroundTint="#FFC107"
    tools:context=".MainActivity">


    <androidx.constraintlayout.widget.Barrier
        android:id="@+id/barrier4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:barrierDirection="left" />

    <androidx.constraintlayout.widget.Barrier
        android:id="@+id/barrier5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:barrierDirection="left" />

    <androidx.constraintlayout.widget.Barrier
        android:id="@+id/barrier6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:barrierDirection="left" />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="488dp"
        android:layout_height="182dp"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="7dp">

        <ImageView
            android:id="@+id/imageView11"
            android:layout_width="703dp"
            android:layout_height="292dp"
            app:srcCompat="@drawable/img_2"
            tools:layout_editor_absoluteX="-90dp"
            tools:layout_editor_absoluteY="-18dp" />

        <ImageView
            android:id="@+id/imageView9"
            android:layout_width="265dp"
            android:layout_height="103dp"
            app:srcCompat="@drawable/b"
            tools:layout_editor_absoluteX="112dp"
            tools:layout_editor_absoluteY="3dp" />

        <TextView
            android:id="@+id/textView7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="#00FFFFFF"
            android:backgroundTintMode="src_in"
            android:fontFamily="@font/abril_fatface"
            android:foregroundTint="#F8F8F8"
            android:text="CoinApp"
            android:textColor="#000000"
            android:textSize="34sp"
            app:drawableTint="#FFFFFF"
            tools:layout_editor_absoluteX="176dp"
            tools:layout_editor_absoluteY="120dp" />


    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="502dp"
        android:layout_height="155dp"
        tools:layout_editor_absoluteX="-5dp"
        tools:layout_editor_absoluteY="200dp">

        <ImageView
            android:id="@+id/imageView12"
            android:layout_width="93dp"
            android:layout_height="91dp"
            app:srcCompat="@drawable/img_3"
            tools:layout_editor_absoluteX="21dp"
            tools:layout_editor_absoluteY="-6dp" />

        <TextView
            android:id="@+id/textView3"
            android:layout_width="102dp"
            android:layout_height="31dp"
            android:fontFamily="sans-serif-black"
            android:text="Bitcoin -"
            android:textColor="#000000"
            android:textSize="24sp"
            tools:layout_editor_absoluteX="21dp"
            tools:layout_editor_absoluteY="92dp" />


        <com.google.android.material.floatingactionbutton.FloatingActionButton
            android:id="@+id/floatingActionButton"
            android:layout_width="39dp"
            android:layout_height="43dp"
            android:clickable="true"
            app:backgroundTint="#FFC107"
            app:fabSize="mini"
            app:srcCompat="@android:drawable/ic_input_add"
            tools:layout_editor_absoluteX="404dp"
            tools:layout_editor_absoluteY="86dp" />

        <ImageView
            android:id="@+id/imageView18"
            android:layout_width="188dp"
            android:layout_height="65dp"
            android:tint="#000000"
            app:srcCompat="@android:drawable/button_onoff_indicator_off"
            tools:layout_editor_absoluteX="200dp"
            tools:layout_editor_absoluteY="78dp" />

        <TextView
            android:id="@+id/textView4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:accessibilityLiveRegion="assertive"
            android:background="#FFC107"
            android:backgroundTint="#00000000"
            android:bufferType="spannable"
            android:drawingCacheQuality="high"
            android:ellipsize="start"
            android:focusable="auto"
            android:fontFamily="sans-serif-black"
            android:shadowRadius="5"
            android:text="19141.4 USS$"
            android:textAlignment="center"
            android:textAppearance="@style/TextAppearance.AppCompat.Display4"
            android:textColor="#FFFFFF"
            android:textCursorDrawable="@color/purple_200"
            android:textDirection="inherit"
            android:textSize="24sp"
            app:circularflow_radiusInDP="10"
            tools:layout_editor_absoluteX="215dp"
            tools:layout_editor_absoluteY="93dp" />


        <ImageView
            android:id="@+id/imageView15"
            android:layout_width="86dp"
            android:layout_height="70dp"
            android:tint="#FFC107"
            app:layout_constraintEnd_toStartOf="@+id/textView4"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toEndOf="@+id/imageView12"
            app:srcCompat="@android:drawable/picture_frame"
            tools:layout_editor_absoluteY="78dp" />

        <TextView
            android:id="@+id/textView6"
            android:layout_width="41dp"
            android:layout_height="38dp"
            android:accessibilityLiveRegion="assertive"
            android:background="#FFC107"
            android:backgroundTint="#00FFC107"
            android:bufferType="spannable"
            android:capitalize="words"
            android:drawingCacheQuality="high"
            android:fontFamily="sans-serif-black"
            android:text="btc"
            android:textAppearance="@style/TextAppearance.AppCompat.Display4"
            android:textColor="#000000"
            android:textCursorDrawable="@color/purple_200"
            android:textSize="24sp"
            tools:layout_editor_absoluteX="136dp"
            tools:layout_editor_absoluteY="92dp" />


    </androidx.constraintlayout.widget.ConstraintLayout>


</androidx.constraintlayout.widget.ConstraintLayout>